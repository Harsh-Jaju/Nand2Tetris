/**
 * 16-bit multiplexor: 
 * for i = 0, ..., 15:
 * if (sel = 0) out[i] = a[i], else out[i] = b[i]
 */
CHIP myMux16 {
    IN a[16], b[16], sel;
    OUT out[16];

    PARTS:
    myMux(a=a[0],b=b[0],sel=sel,out=out[0]);
    myMux(a=a[1],b=b[1],sel=sel,out=out[1]);
    myMux(a=a[2],b=b[2],sel=sel,out=out[2]);
    myMux(a=a[3],b=b[3],sel=sel,out=out[3]);
    myMux(a=a[4],b=b[4],sel=sel,out=out[4]);
    myMux(a=a[5],b=b[5],sel=sel,out=out[5]);
    myMux(a=a[6],b=b[6],sel=sel,out=out[6]);
    myMux(a=a[7],b=b[7],sel=sel,out=out[7]);
    myMux(a=a[8],b=b[8],sel=sel,out=out[8]);
    myMux(a=a[9],b=b[9],sel=sel,out=out[9]);
    myMux(a=a[10],b=b[10],sel=sel,out=out[10]);
    myMux(a=a[11],b=b[11],sel=sel,out=out[11]);
    myMux(a=a[12],b=b[12],sel=sel,out=out[12]);
    myMux(a=a[13],b=b[13],sel=sel,out=out[13]);
    myMux(a=a[14],b=b[14],sel=sel,out=out[14]);
    myMux(a=a[15],b=b[15],sel=sel,out=out[15]);
}
