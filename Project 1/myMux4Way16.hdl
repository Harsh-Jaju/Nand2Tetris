// File name: projects/1/Mux4Way16.hdl
/**
 * 4-way 16-bit multiplexor:
 * out = a if sel = 00
 *       b if sel = 01
 *       c if sel = 10
 *       d if sel = 11
 */
CHIP myMux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];
    
    PARTS:
    
    myMux16(a=a,b=b,sel=sel[0],out=x1);
    myMux16(a=c,b=d,sel=sel[0],out=x2);
    myMux16(a=x1,b=x2,sel=sel[1],out=out);
}