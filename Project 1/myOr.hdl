/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP myOr {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    myNot(in=a,out=a1);
    myNot(in=b,out=b1);
    myAnd(a=a1,b=b1,out=c1);
    myNot(in=c1,out=out);
}
